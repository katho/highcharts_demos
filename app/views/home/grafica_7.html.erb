		<%= javascript_include_tag "jquery.min", "highcharts", "exporting", "data" %>
		<script type="text/javascript">
$(function () {
    $('#container').highcharts({
        data: {
            table: document.getElementById('mi_tabla')
        },
        chart: {
            type: 'column'
        },
        title: {
            text: 'Formas de agresión por parte de los programadores en la última junta'
        },
        yAxis: {
            allowDecimals: false,
            title: {
                text: 'Unidades'
            }
        },
        tooltip: {
            formatter: function() {
                return '<b>'+ this.series.name +'</b><br/>'+
                    this.y +' '+ this.x.toLowerCase();
            }
        }
    });
});
		</script>
		<!-- Replace favicon.ico & apple-touch-icon.png in the root of your domain and delete these references -->
